{
  "version": 1,
  "projects": {
    "angular8": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular8",
      "sourceRoot": "apps/angular8/src",
      "prefix": "my-angular8",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular8",
            "index": "apps/angular8/src/index.html",
            "main": "apps/angular8/src/main.ts",
            "polyfills": "apps/angular8/src/polyfills.ts",
            "tsConfig": "apps/angular8/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/angular8/src/favicon.ico",
              "apps/angular8/src/assets"
            ],
            "styles": ["apps/angular8/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular8/src/environments/environment.ts",
                  "with": "apps/angular8/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular8:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular8:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular8:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular8/tsconfig.app.json",
              "apps/angular8/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/angular8/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angular8/jest.config.js",
            "tsConfig": "apps/angular8/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/angular8/src/test-setup.ts"
          }
        }
      }
    },
    "angular8-e2e": {
      "root": "apps/angular8-e2e",
      "sourceRoot": "apps/angular8-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular8-e2e/cypress.json",
            "tsConfig": "apps/angular8-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular8:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular8:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/angular8-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/angular8-e2e/**"]
          }
        }
      }
    },
